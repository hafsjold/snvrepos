<?xml version="1.0" encoding="utf-8" ?> 
<Sftp>
  <Status>{{status}}</Status>
  {% if status %}
  <Id>{{sftp.Id}}</Id> 
  <Navn>{{sftp.Navn}}</Navn> 
  <Host>{{sftp.Host}}</Host> 
  <Port>{{sftp.Port}}</Port>
  <User>{{sftp.User}}</User> 
  <Outbound>{{sftp.Outbound}}</Outbound> 
  <Inbound>{{sftp.Inbound}}</Inbound> 
  <Pincode>{{sftp.Pincode|escape}}</Pincode> 
  <Certificate>{{sftp.Certificate|escape}}</Certificate>
  {% endif %}  
</Sftp>

  
