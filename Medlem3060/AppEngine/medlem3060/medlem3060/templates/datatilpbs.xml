<?xml version="1.0" encoding="utf-8" ?> 
<Datatilpbs>
  <Sendqueue>
    <Id>{{sendqueue.Id}}</Id>
  </Sendqueue>
  <Sftp>
    <Id>{{sftp.Id}}</Id> 
    <Navn>{{sftp.Navn}}</Navn> 
    <Host>{{sftp.Host}}</Host> 
    <Port>{{sftp.Port}}</Port> 
    <User>{{sftp.User}}</User> 
    <Outbound>{{sftp.Outbound}}</Outbound> 
    <Inbound>{{sftp.Inbound}}</Inbound> 
    <Pincode>{{sftp.Pincode}}</Pincode> 
    <Certificate>{{sftp.Certificate}}</Certificate> 
  </Sftp>
  <Pbsfile>
    <Id>{{pbsfile.Id}}</Id> 
    <TilPBSFilename>{{pbsfile.TilPBSFilename}}</TilPBSFilename> 
    <Transmisionsdato>{{pbsfile.Transmisionsdato}}</Transmisionsdato> 
    <SendData>{{pbsfile.SendData|escape}}</SendData> 
  </Pbsfile>
</Datatilpbs>